<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="isPreventDoubleClick" format="boolean" />

    <declare-styleable name="ACBAEditText">
        <attr name="errorMessage" format="string" />
        <attr name="layoutId" format="reference" />
        <attr name="minLength" format="integer" />
        <attr name="regex" format="string" />
        <attr name="validator" format="enum">
            <enum name="none" value="0" />
            <enum name="email" value="1" />
            <enum name="password" value="2" />
            <enum name="regex" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ACBAETextInput" />

    <declare-styleable name="ACBATextView">
        <attr name="isPreventDoubleClick" />
    </declare-styleable>

    <declare-styleable name="ACBAImageView">
        <attr name="isPreventDoubleClick" />
    </declare-styleable>

    <declare-styleable name="ACBAToolBar" />

    <declare-styleable name="ACBAButton">
        <attr name="isPreventDoubleClick" />
    </declare-styleable>
</resources>